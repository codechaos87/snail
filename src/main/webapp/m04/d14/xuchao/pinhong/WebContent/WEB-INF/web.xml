<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>pinhong</display-name>
  <servlet>
    <servlet-name>pinhongservlet</servlet-name>
    <servlet-class>main.java.pinhong.controller.PinHongServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pinhongservlet</servlet-name>
    <url-pattern>/pinhong</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>pinhong</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginservlet</servlet-name>
    <servlet-class>main.java.pinhong.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginservlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>productservlet</servlet-name>
    <servlet-class>main.java.pinhong.controller.ProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productservlet</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>backservlet</servlet-name>
    <servlet-class>main.java.pinhong.controller.BackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>backservlet</servlet-name>
    <url-pattern>/back</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>nextservlet</servlet-name>
    <servlet-class>main.java.pinhong.controller.NextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>nextservlet</servlet-name>
    <url-pattern>/next</url-pattern>
  </servlet-mapping>
</web-app>